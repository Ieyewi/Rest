Player.Chatted:Connect(function(cht)
    if cht:lower():match("cmds") then
        local notification = NotificationService:Notify("Load Done", "Please Say /console", "", 0, false, "Ok")
        notification.Button1.Event:Connect(function()
            print [[Alex Admins To Use Command Say"(Commands)" On Chat you can hide the command by saying "/e (Commands)
            COMMANDS:
            -Fly
            -Fe Gui
            -IY
            -Fake Chat
            
            Function:
            -Fly (an Fly Gui)
            -Fe Gui (an Gui That Has Much thing that almost all is fe)
            -IY (Basically Admin Script its Infinite Yield)
            -Fake Chat (An Gui That can create fake chat
            ]]

            Player.Chatted:Connect(function(cht)
                if cht:lower():match("fly") then
                    loadstring(game:HttpGet("https://pastebin.com/raw/w3Bs9rHN"))();
                end
            end)

            Player.Chatted:Connect(function(cht)
                if cht:lower():match("fe gui") then
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/Blukez/Scripts/main/UTG%20V3%20RAW"))();
                end
            end)

            Player.Chatted:Connect(function(cht)
                if cht:lower():match("fake chat") then
                    loadstring(game:HttpGet('https://pastebin.com/raw/MBpnp3yS'))()
                end
            end)
        end)
    end
end)
